isFeed=true
resultPattern=valign="middle"><a href\=\"(http://www\.donationcoder\.com/Forums/bb/index\.php\?topic\=[\.\d]*;topicseen)\">(.*?)</a>.*\?board=[\.\d]*">(.*)</a>.*\s\s.*\s.*?<a href=\".*?\">(.*?)</a></td>\s.*\s.*\s.*\s.*\s.*\s\s.*\s.*\s*(<b>Today</b> at |\d\d \w* \d\d\d\d, )(.*)<br />\s.*by <a href=\".*?\">(.*?)</a>
farrCaption=$2
farrGroup=$3, started by $4, last post $5$6 by $7
farrPath=$1
